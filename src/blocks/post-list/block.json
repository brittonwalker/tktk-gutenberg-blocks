{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "tktk-blocks/post-list",
	"version": "0.1.0",
	"title": "TKTK Post List",
	"description": "A block that displays the latest posts of a selected post type",
	"category": "tktk-blocks",
	"attributes": {
		"title": {
			"type": "string"
		},
		"postType": {
			"type": "string",
			"default": "post"
		}
	},
	"supports": {
		"html": false
	},
	"textdomain": "tktk-blocks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
